{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "taxgenienew"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/vendors/bower_components/jquery-toast-plugin/dist/jquery.toast.min.css",
        "assets/vendors/bower_components/fullcalendar/dist/fullcalendar.css",
        "assets/vendors/bower_components/datatables/media/css/jquery.dataTables.min.css",
        "assets/vendors/bower_components/bootstrap-select/dist/css/bootstrap-select.min.css",
        "assets/dist/css/style.css",
        "styles.css"
      ],
      "scripts": [
        "assets/vendors/bower_components/jquery/dist/jquery.min.js",
        "assets/vendors/bower_components/bootstrap/dist/js/bootstrap.min.js",
        "assets/vendors/bower_components/datatables/media/js/jquery.dataTables.min.js",
		    "assets/dist/js/dataTables-data.js",
        "assets/dist/js/jquery.slimscroll.js",
        "assets/dist/js/dropdown-bootstrap-extended.js",
        "assets/vendors/bower_components/jquery-toast-plugin/dist/jquery.toast.min.js",
        "assets/vendors/bower_components/moment/min/moment.min.js",
        "assets/vendors/jquery-ui.min.js",
        "assets/vendors/bower_components/fullcalendar/dist/fullcalendar.min.js",
        "assets/dist/js/fullcalendar-data.js",
        "assets/vendors/bower_components/bootstrap-select/dist/js/bootstrap-select.min.js",
        "assets/vendors/bower_components/jasny-bootstrap/dist/js/jasny-bootstrap.min.js",
        "assets/dist/js/init.js",
        "assets/vendors/bower_components/waypoints/lib/jquery.waypoints.min.js",
        "assets/vendors/bower_components/jquery.counterup/jquery.counterup.min.js",
        "assets/dist/js/dashboard2-data.js"
       
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts", 
        "staging": "environments/environment.staging.ts", 
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
